{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowSSLRequestsOnly",
      "Effect": "Deny",
      "Principal": "*",
      "Action": "s3:*",
      "Resource": [
        "arn:aws:s3:::levantafia-dev-photos",
        "arn:aws:s3:::levantafia-dev-photos/*"
      ],
      "Condition": {
        "Bool": {
          "aws:SecureTransport": "false"
        }
      }
    },
    {
      "Sid": "AllowCloudFrontOAI",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::cloudfront:user/CloudFront Origin Access Identity E1TJARB0N6RP51"
      },
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::levantafia-dev-photos/*"
    }
  ]
}
